{
    "name": "docuseal/docuseal-laravel",
    "description": "Laravel SDK for Docuseal",
    "keywords": [
        "Docuseal",
        "laravel",
        "docuseal-laravel"
    ],
    "homepage": "https://github.com/docuseal/docuseal-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Abishek R Srikaanth",
            "email": "abishek@technicaliti.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^10.0||^11.0",
        "saloonphp/saloon": "^3.0",
        "spatie/laravel-data": "^4.6",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "spatie/laravel-ray": "^1.35"
    },
    "autoload": {
        "psr-4": {
            "DocusealCo\\Docuseal\\": "src/",
            "DocusealCo\\Docuseal\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DocusealCo\\Docuseal\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-docuseal-laravel --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "lint": [
            "shift-cli",
            "pint",
            "vendor/bin/phpstan analyse"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DocusealCo\\Docuseal\\DocusealServiceProvider"
            ],
            "aliases": {
                "Docuseal": "DocusealCo\\Docuseal\\Facades\\Docuseal"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
